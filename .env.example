# Path to the application that will run inside Docker.
# Relative paths are allowed.
PROJECT_PATH=../my-app

# Application environment variables
APP_ENV=local
APP_PORT=80

# PostgreSQL credentials
POSTGRES_USER=app
POSTGRES_PASSWORD=secret
POSTGRES_DB=app_db

# Optional custom ports
POSTGRES_PORT=5432
REDIS_PORT=6379
PHP_FPM_PORT=9000
NGINX_PORT=80
